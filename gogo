

cat > /root/net <<EOF
netstat  -anplut
EOF


cat > /root/fhq <<EOF
iptables -nxvL

EOF


cat > /root/ruisu <<EOF
/appex/bin/serverSpeeder.sh status

EOF

cat > /root/ruisu <<EOF
/appex/bin/serverSpeeder.sh status

EOF


cat > /root/ruisur <<EOF
#/serverspeeder/bin/serverSpeeder.sh  stop
/appex/bin/serverSpeeder.sh    stop 
echo 3333333333
sleep 3
#/serverspeeder/bin/serverSpeeder.sh  start
/appex/bin/serverSpeeder.sh    start 
EOF
chmod +x /root/ruisur
wget -N --no-check-certificate "https://raw.githubusercontent.com/vvkkjj/Linux-NetSpeed/master/tcp.sh" && chmod +x tcp.sh

wget -N --no-check-certificate "https://github.com/vvkkjj/Linux-NetSpeed/raw/master/bbr/iproute_4.3.0-1ubuntu3.16.04.4_all.deb"
wget -N --no-check-certificate "https://github.com/vvkkjj/Linux-NetSpeed/raw/master/bbr/suanvv.deb"
sleep 1
dpkg -i /root/iproute_4.3.0-1ubuntu3.16.04.4_all.deb
sleep 1
dpkg -i /root/suanvv.deb
sleep 3
/etc/init.d/shadowvpn stop
mv /etc/init.d/shadowvpn  /etc/init.d/shadowvpn-
/etc/init.d/shadowvpn start



